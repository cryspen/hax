  let o3 : Bool ←
    (Core.Option.Impl.is_none_or i32 (i32 -> RustM Bool)
      (← (Core.Clone.Clone.clone o1))
      (fun x => (do
        (Rust_primitives.Hax.Machine_int.eq x (0 : i32)) : RustM Bool)));
