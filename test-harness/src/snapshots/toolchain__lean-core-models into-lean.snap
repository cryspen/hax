  -- no fields

inductive E2 : Type
| C1 : E2
| C2 : u32 -> E2

def tests
  (_ : Rust_primitives.Hax.Tuple0)
  : RustM (Core.Result.Result u32 E1)
  := do
  let v1 : (Core.Result.Result u32 E1) := (Core.Result.Result.Ok (1 : u32));
  let v2 : (Core.Result.Result u32 E1) := (Core.Result.Result.Err E1.C1);
  let f : (u32 -> RustM u32) := (fun x => (do (x +? (1 : u32)) : RustM u32));
  let v5 : (Core.Result.Result i32 E1) ←
    (Core.Result.Impl.map i32 E1 i32 (i32 -> RustM i32)
